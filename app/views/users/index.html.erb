<div class = "container">
  <div class = "row">
   <%= render 'books/list', book: @book, user_id: @user_id %>
    <div class = "col-lg-8">
      <h1>User</h1>
      <table class="table table-striped">
        <tbody>
          <tr>
            <th>image</th>
            <th>name</th>
          </tr>
          <% @users.each do |f| %>
           <tr>
            <td><%= image_tag .get_profile_image(100,100) %></td>
            <td>
                <%= f.name  %>
            </td>
            <td>
                <%= link_to 'Show', user_path(f) %>
            </td>
           </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>